        <int8_t name='tree_drown_level'/>
        <int8_t name='sapling_drown_level'/>
    </struct-type>

    <struct-type type-name='material_vec_ref'>
        <stl-vector name='material'>
            <enum base-type='int16_t' type-name='material_type'/>
        </stl-vector>
        <stl-vector name='matgloss'>
            <int16_t refers-to='(matgloss-target $$._parent._parent.material[$$._key] $)'/>
        </stl-vector>
    </struct-type>

    <struct-type type-name='contaminant'>
        <int16_t name='type'/>
        <enum base-type='int16_t' name='material' type-name='material_type'/>
        <int16_t name='matgloss' refers-to='(matgloss-target $$._parent.material $)'/>
        <int32_t name='amount'/>
    </struct-type>

    <bitfield-type type-name='matgloss_blood' base-type='uint8_t'>
        <flag-bit name='blood'/>
        <flag-bit name='blood_g'/>
        <flag-bit name='blood_b'/>
        <flag-bit name='pus'/>
        <flag-bit name='blood_m'/>
        <flag-bit name='blood_c'/>
        <flag-bit name='ichor'/>
        <flag-bit name='goo'/>
    </bitfield-type>
</data-definition>

