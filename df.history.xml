                <int32_t name='site' ref-target='world_site'/>
            </vmethod>

            <vmethod ret-type='bool' name='wasArtifactHidden'>
                <int32_t name='artifact' ref-target='artifact_record'/>
            </vmethod>
            <vmethod name='getArtifactMakerUnitID'>
